{
  "GET": [
    {
      "path": "/",
      "type": "static",
      "entrypoint": "public/landing/"
    },
    {
      "path": "/api/user/:id",
      "middlewares": ["authorize", "database"],
      "func": "user"
    },
    {
      "path": "/api/users",
      "type": "proxy",
      "endpoint": "https://jsonplaceholder.typicode.com/users"
    }
  ],
  "POST": [
    {
      "path": "/auth",
      "middlewares": ["authenticate"],
      "func": "authenticate"
    },
    {
      "path": "/api/user",
      "middlewares": ["authorize"],
      "func": "user"
    }
  ],
  "PUT": [],
  "DELETE": []
}
